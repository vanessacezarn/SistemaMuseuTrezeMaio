<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="493.0" prefWidth="580.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eglv.sistemagerenciamentoacervos.controller.JornalExcluirController">
    <children>
        <Label fx:id="lblEditar" layoutX="191.0" layoutY="14.0" text="EXCLUIR JORNAL">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <Label fx:id="lblBusca" layoutX="20.0" layoutY="61.0" text="BUSCAR POR :">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <TextField fx:id="txtBusca" layoutX="279.0" layoutY="96.0" prefHeight="35.0" prefWidth="203.0" />
        <TableView fx:id="tblJornal" layoutX="23.0" layoutY="163.0" prefHeight="307.0" prefWidth="533.0">
            <columns>
                <TableColumn fx:id="colCodigo" minWidth="0.0" prefWidth="187.0" text="CÓDIGO" />
                <TableColumn fx:id="colTitulo" minWidth="-1.0" prefWidth="345.0" text="TÍTULO" />
            </columns>
        </TableView>
        <Label fx:id="lblEscolha" layoutX="81.0" layoutY="140.0" text="SELECIONE NA TABELA ABAIXO O JORNAL QUE DESEJA EXCLUIR." />
        <ComboBox fx:id="cmbEscolha" layoutX="139.0" layoutY="60.0" prefHeight="30.0" prefWidth="241.0" />
        <Label fx:id="lblDigite" layoutX="23.0" layoutY="101.0" text="DIGITE O TERMO PARA PESQUISA :">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <Button fx:id="btnSair" layoutX="482.0" layoutY="36.0" mnemonicParsing="false" onAction="#sair" text="SAIR" />
        <Button fx:id="btnExcluir" layoutX="493.0" layoutY="128.0" mnemonicParsing="false" onAction="#btnExcluir" text="EXCLUIR" />
    </children>
</AnchorPane>
